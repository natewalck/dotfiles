[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
    changed = diff --name-only HEAD^1
[color]
    ui = true
[core]
    excludesfile = ~/.gitignore
    editor = /usr/bin/vi

[hub]
    protocol = https

[push]
    default = simple
[includeIf "gitdir:~/code/"]
  path = ~/code/dotfiles/.gitconfig-personal
[includeIf "gitdir:~/code/square/"]
  path = ~/code/dotfiles/.gitconfig-work

[init]
	defaultBranch = main
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
[user]
	name = Nate Walck
	email = github@natewalck.com
